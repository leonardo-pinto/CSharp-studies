@*strongly typed view*@
@model ListModel

@*Example using Strongly Typed Views*@
<div class="list-container">
    <h3>@Model.ListTitle</h3>
    <ul class="list">
        @foreach (string item in Model.ListItems)
        {
            <li>@item</li>
        }
    </ul>
</div>


@*Example using ViewData or ViewBag
<div class="list-container">
   <h3>@ViewData["ListTitle"]</h3>
    <h3>@ViewBag.ListTitle</h3>
    <ul class="list">
        @foreach (string item in ViewBag.ListItems)
        {
            <li>@item</li>
        }
    </ul>
</div>*@